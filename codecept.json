{
  "output": "./output",
  "helpers": {
    "WebDriverIO": {
      "url": "https://katalon-demo-cura.herokuapp.com",
      "host": "localhost",
      "port": "4444",
      "protocol": "http",
      "path": "/wd/hub",
      "smartWait": 5000,
      "uniqueScreenshotNames": true,
      "windowSize": "maximize",
      "browser": "chrome"
    },
    "MyHelper": {
      "require": "./myhelper_helper.js"
    }
  },
  "include": {
    "I": "./steps_file.js"
  },
  "mocha": {
